{
    "name": "iadambrown/laravel-helpers",
    "homepage": "https://github.com/iadambrown/laravel-helpers",
    "description": "A list of personal helper functions for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Brown",
            "email": "iadambrown@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "fzaninotto/faker": "^1.8",
        "illuminate/support": "^5.7|6.0.*",
        "mockery/mockery": "dev-master",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "iAdamBrown\\LaravelHelpers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iAdamBrown\\LaravelHelpers\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "iAdamBrown\\LaravelHelpers\\LaravelHelpersServiceProvider"
            ]
        }
    }
}
